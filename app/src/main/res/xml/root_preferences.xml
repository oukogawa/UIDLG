<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="設定">

        <SwitchPreferenceCompat
            app:key="enablevideo"
            app:title="カメラ有効"
            app:defaultValue="false" />
        <EditTextPreference
            app:key="username"
            app:title="ユーザ ID"
            app:dialogTitle="ユーザ ID"
            app:defaultValue="test-pattern00"
            app:useSimpleSummaryProvider="true" />
        <SwitchPreferenceCompat
            app:key="enabledebug"
            app:title="テストサーバー"
            app:defaultValue="false" />

    </PreferenceCategory>


</PreferenceScreen>